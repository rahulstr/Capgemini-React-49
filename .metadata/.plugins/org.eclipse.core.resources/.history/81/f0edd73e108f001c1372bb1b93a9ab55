package com.BasichandsOn;

class student1{
	public String toString() {
		return "Student: Name= " + name +'\n'+ " Rollno= " + rn +'\n'+ " Standard= " + std +'\n'+ " Mobile= " + mob + " ";
	}
	
	public student1(String name,int rn, int std,int mob) {
		this.name=name;
		this.rn=rn;
		this.std=std;
		this.mob=mob;
	}
	
	private String name;
	private int rn;
	private int std;
	private int mob;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getRn() {
		return rn;
	}

	public void setRn(int rn) {
		this.rn = rn;
	}

	public int getStd() {
		return std;
	}

	public void setStd(int std) {
		this.std = std;
	}

	public int getMob() {
		return mob;
	}

	public void setMob(int mob) {
		this.mob = mob;
	}

}

public class student {

	public static void main(String[] args) {
		
		student1 s=new student1("Rahul", 454 , 5 , 998877665);
		
		System.out.println(s);

	}

}
