package com.selfstudy;

public class nine {
	
	public static void main(String[] args) {
		boolean gameover=true;
		int score =800;
		int lcomp=5;
		int bonus=100;
		
		calscrmethod(gameover, score, lcomp, bonus);
		
	}
	
	public static void calscrmethod(boolean gameover,int score,int lcomp,int bonus) {
		
		if(gameover) {
			int finalscore=score+(lcomp*bonus);
			finalscore+=2000;
			System.out.println("final score is "+finalscore);
			
			
		}
	}
	
}