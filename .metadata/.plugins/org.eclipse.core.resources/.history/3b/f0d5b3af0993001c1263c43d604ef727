package capgday3;

public class Strint {

	public static void main(String[] args) {
		String s="200";
		int i=Integer.parseInt(s);
		
		System.out.print("Integer Value of String s is : ");
		System.out.println(i);
		
		int a;
		a=i+200;
		
		System.out.print("Sum of Int i and 200 is :");
		System.out.println(a);

	}

}
